{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId"
    ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "C:\\Temp\\Logs\\log.txt",
          "outputTemplate": "{Timestamp} {Message} {NewLine:1} {Exception:1}"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:8081"
        }
      }
    ]
  },
  "ConnectionStrings": {
    //    "DatabaseConnectionPostrgreSQL": "Server=localhost;Port=5432;Database=Scout_dev;User Id=postgres;Password=!23Haslo;",
  },
  "AllowedHosts": "*"
}
